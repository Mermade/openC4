        <!-- Start Branding script tag -->
        <script type="text/javascript">
            // get the branding logo to set the mouseover/out and onclick
            var brandingLogo = document.getElementById('branding-logo');
            if(brandingLogo != undefined) {
                brandingLogo['onclick'] = function() {
                    location.href = "http://developer.channel4.com/";
                };
                brandingLogo['onmouseover'] = function() {
                    this.style.cursor = 'pointer';
                };
                brandingLogo['onmouseout'] = function() {
                    this.style.cursor = 'auto';
                };
            }

            // get the mashery logo to set the mouseover/out and onclick
            var masheryLogo = document.getElementById('branding-mashery');
            if(masheryLogo != undefined) {
                masheryLogo['onclick'] = function() {
                    location.href = "http://www.mashery.com/";
                };
                masheryLogo['onmouseover'] = function() {
                    this.style.cursor = 'pointer';
                };
                masheryLogo['onmouseout'] = function() {
                    this.style.cursor = 'auto';
                };
            }
        </script>
        <!-- End Branding script tag -->
            <!-- <div id="extra1">&nbsp;</div>
            <div id="extra2">&nbsp;</div> -->

        <script type="text/javascript">
//<![CDATA[
/* ##############################
 * # Javascript Hack to insert  #
 * # custom text into App       #
 * # registration form for      #
 * # Channel4.                  #
 * #                            #
 * # By: Kelly at Mashery       #
 * ############################## */
 
(function() {
    if(window.location.pathname == '/apps/register' || /^\/apps\/edit.*/.test(window.location.pathname)) {
        var form = document.getElementById('application-edit'); // App registration form
        var welcome = document.createElement('p');
        welcome.appendChild(document.createTextNode('In order for us to assess your application we’d appreciate it if you told us about you and your proposed use of the Channel 4 API and Channel 4 Content.'));
        form.parentNode.insertBefore(welcome, form); // insert welcome message before the form.
        
        var fieldsets = form.getElementsByTagName('fieldset');
        
        var mainFieldset = fieldsets[0];
        
        var formDL = mainFieldset.getElementsByTagName('dl')[0];
        
        var labels = formDL.getElementsByTagName('label');
        
        var DTs = formDL.getElementsByTagName('dt');
        
        var websiteHeaderDT = document.createElement('dt');
        
        var websiteHeader = document.createElement('h2');
        websiteHeader.appendChild(document.createTextNode('Your Website (your "Site")'));
        
        websiteHeaderDT.appendChild(websiteHeader);
        
        formDL.insertBefore(websiteHeaderDT, DTs[0]);
        
        for (var x = 0; x < labels.length; x++) {
            if (labels[x].getAttributeNode('for') != undefined) {
                if (labels[x].getAttributeNode('for').value == 'applications-notes') {
                    var purposeDT = document.createElement('dt');
                    
                    var purposeHeader = document.createElement('h2');
                    purposeHeader.appendChild(document.createTextNode('Purpose for use of the Channel 4 API/Channel 4 Content ("Purpose")'));
                    
                    purposeDT.appendChild(purposeHeader);
                    
                    formDL.insertBefore(purposeDT, labels[x].parentNode);
                }
            }
        }
    }
})();
//]]>
</script>
		
